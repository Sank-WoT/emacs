var a = 2 + 2;

switch (a) {
  case 3:
    alert( 'Маловато' );
  case 4:
    alert( 'В точку!' );
  case 5:
    alert( 'Перебор' );
  default:
    alert( 'Я таких значений не знаю' );
}
function showMessage() {
  alert( 'Привет всем присутствующим!' );
}

function showMessage2() {
    alert("sank");
}

function showMessage3() {
    allert("Hello".toLowerCase);
}
